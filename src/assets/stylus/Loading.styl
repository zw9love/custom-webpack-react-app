.loading-container
  position fixed
  left 50%
  top 50%
  transform translate(-50%,-50%)
  width 100%
  height 100%
  background-color rgba(0,0,0,.6)
  z-index 1111
  text-align center
  > div
    position absolute
    left 50%
    top 50%
    transform translate(-50%,-50%)
    p
      color #fff
      font-size 14px

